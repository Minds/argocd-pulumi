    "files": [
        "accountToken.ts",
        "application.ts",
        "applicationSet.ts",
        "cluster.ts",
        "config/index.ts",
        "config/vars.ts",
        "index.ts",
        "project.ts",
        "projectToken.ts",
        "provider.ts",
        "repository.ts",
        "repositoryCertificate.ts",
        "repositoryCredentials.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts"
    ]
}
